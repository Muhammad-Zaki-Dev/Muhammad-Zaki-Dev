@model List<OnlineShoppingMVC.Models._Color>
<table id="datatable" class="table table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
    <thead>
        <tr>
            <th>Color Name</th>
           
            <th>Status</th>
            <th>Edit </th>
            <th>Delete</th>
        </tr>
    </thead>


    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Color</td>
                <td>@item.Status</td>
                <td><a href="#" onclick="EditColor(@item.ColorID);" class="btn btn-danger"><i class="fa fa-edit"></i></a></td>
                <td><a href="#" onclick="DeleteColor(@item.ColorID);" class="btn btn-primary"><i class="fa fa-trash"></i></a></td>


            </tr>
        }



    </tbody>
</table>
