@model List<OnlineShoppingMVC.Models._SubCategory>
<table id="datatable" class="table table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
    <thead>
        <tr>
            <th>SubCategory Name</th>
            <th>Category Name</th>

            <th>Image</th>
            <th>Status</th>
            <th>Edit </th>
            <th>Delete</th>
        </tr>
    </thead>


    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.SubCategoryName</td>
                <td>@item.CategoryName</td>
                <td><img src='@item.SubCategoryImage' alt="img" width="150px" height="60px" /></td>
                <td>@item.Status</td>
                <td><a href="#" onclick="EditSubCategory(@item.SubCategoryID);" class="btn btn-danger"><i class="fa fa-edit"></i></a></td>
                <td><a href="#" onclick="DeleteSubCategory(@item.SubCategoryID);" class="btn btn-primary"><i class="fa fa-trash"></i></a></td>


            </tr>
        }



    </tbody>
</table>
